{
  "name": "flowmap.gl",
  "description": "Flow map drawing layer for deck.gl",
  "author": "Teralytics AG",
  "license": "Apache-2.0",
  "version": "4.0.0",
  "keywords": [
    "flows",
    "flow map",
    "webgl",
    "visualization",
    "overlay",
    "layer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/teralytics/flowmap.gl.git"
  },
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "dist-esm"
  ],
  "scripts": {
    "start": "start-storybook -p 6006 -s ./public",
    "storybook:build": "build-storybook -s ./public",
    "storybook:deploy": "npm run storybook:build && gh-pages -d storybook-static",
    "build:es5": "rm -rf dist && tsc --project tsconfig.build.json",
    "build:esm": "rm -rf dist-esm && tsc --project tsconfig.build.esm.json",
    "build": "npm-run-all build:*",
    "typecheck": "tsc --noEmit",
    "lint": "tslint '{src,test,stories}/**/*.{ts,tsx}'",
    "prepare": "npm run build"
  },
  "dependencies": {
    "d3-array": "^2.0.2",
    "d3-collection": "^1.0.7",
    "d3-color": "^1.2.3",
    "d3-interpolate": "^1.3.2",
    "d3-scale": "^2.1.2",
    "lodash": "^4.17.11",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@mapbox/geo-viewport": "^0.4.0",
    "@storybook/addon-options": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "@types/d3-array": "^1.2.4",
    "@types/d3-collection": "^1.0.7",
    "@types/d3-color": "^1.2.1",
    "@types/d3-dsv": "^1.0.34",
    "@types/d3-geo": "^1.10.3",
    "@types/d3-interpolate": "^1.3.0",
    "@types/d3-scale": "^2.0.2",
    "@types/dom4": "^2.0.1",
    "@types/geojson": "^7946.0.4",
    "@types/gl-matrix": "^2.4.4",
    "@types/lodash": "^4.14.119",
    "@types/mapbox-gl": "^0.51.1",
    "@types/mapbox__geo-viewport": "^0.3.1",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-map-gl": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "@types/viewport-mercator-project": "^5.2.1",
    "@types/webpack": "^4.4.21",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "d3-dsv": "^1.0.10",
    "d3-geo": "^1.11.3",
    "deck.gl": "^6.3.2",
    "dotenv-webpack": "^1.5.7",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "luma.gl": "^6.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-map-gl": "^4.0.5",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1"
  },
  "peerDependencies": {
    "react": "0.14.x - 16.x",
    "react-dom": "0.14.x - 16.x",
    "deck.gl": ">= 6.0.0",
    "luma.gl": ">= 6.0.0"
  },
  "lint-staged": {
    "{src,test,stories}/**/*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & npm run typecheck"
    }
  }
}
