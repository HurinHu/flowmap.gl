{
  "name": "@tera/flowmap.gl",
  "description": "Flow map drawing layer for deck.gl",
  "author": "Teralytics AG",
  "license": "Apache-2.0",
  "version": "3.0.0-alpha.10",
  "keywords": [
    "flows",
    "flow map",
    "webgl",
    "visualization",
    "overlay",
    "layer"
  ],
  "publishConfig": {
    "registry": "https://api.bintray.com/npm/teralytics/internal-npm/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teralytics/flow-map.gl.git"
  },
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist-esm"
  ],
  "scripts": {
    "start": "STORYBOOK_MapboxAccessToken=$MapboxAccessToken start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "build:es5": "rm -rf dist && tsc --project tsconfig.build.json",
    "build:esm": "rm -rf dist-esm && tsc --project tsconfig.build.esm.json",
    "build": "npm-run-all build:*",
    "typecheck": "tsc --noEmit",
    "lint": "tslint '{src,test,stories}/**/*.{ts,tsx}'",
    "precommit": "lint-staged & npm run typecheck",
    "prepare": "npm run build"
  },
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-collection": "^1.0.4",
    "d3-color": "^1.0.3",
    "d3-interpolate": "^1.1.6",
    "d3-scale": "^2.0.0",
    "lodash": "^4.17.5",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@mapbox/geo-viewport": "^0.2.2",
    "@storybook/react": "^3.3.13",
    "@types/d3-array": "^1.2.1",
    "@types/d3-collection": "^1.0.5",
    "@types/d3-color": "^1.0.5",
    "@types/d3-interpolate": "^1.1.6",
    "@types/d3-scale": "^2.0.0",
    "@types/geojson": "^7946.0.1",
    "@types/gl-matrix": "^2.4.0",
    "@types/lodash": "^4.14.104",
    "@types/mapbox-gl": "^0.43.3",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/storybook__react": "^3.0.7",
    "@types/webpack": "^3.8.8",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "deck.gl": "^5.1.0",
    "file-loader": "^1.1.7",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "luma.gl": "^5.1.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-map-gl": "^3.2.4",
    "style-loader": "^0.20.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.8.0",
    "tslint-loader": "^3.5.3",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0"
  },
  "peerDependencies": {
    "react": "0.14.x - 16.x",
    "react-dom": "0.14.x - 16.x",
    "deck.gl": "5.0.x - 5.1.x",
    "luma.gl": "5.0.x - 5.1.x"
  },
  "lint-staged": {
    "{src,test,stories}/**/*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  }
}
